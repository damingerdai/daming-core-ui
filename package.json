{
  "name": "daming-core-ui",
  "version": "1.0.2",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production",
    "changelog": "conventiona-l-changelog -p angular -i CHANGELOG.md -s",
    "changelog:all": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "sass": "sass-lint -v -q",
    "push": "ng build --configuration production --baseHref=/daming-core-ui/ && npx angular-cli-ghpages --dir=dist/daming-core-ui",
    "push:ci": "ng deploy --no-silent",
    "postinstall": "ngcc --properties es2015 browser module main --first-only",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "version:patch": "yarn version --patch",
    "version:minor": "yarn version --minor",
    "prepare": "is-ci || husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.2.13",
    "@angular/common": "~12.2.13",
    "@angular/compiler": "~12.2.13",
    "@angular/core": "~12.2.13",
    "@angular/forms": "~12.2.13",
    "@angular/platform-browser": "~12.2.13",
    "@angular/platform-browser-dynamic": "~12.2.13",
    "@angular/router": "~12.2.13",
    "@coreui/coreui": "~2.1.16",
    "@coreui/coreui-plugin-chartjs-custom-tooltips": "^1.3.1",
    "angular2-toaster": "^11.0.1",
    "bootstrap": "^4.5.0",
    "chart.js": "^2.9.3",
    "core-js": "^3.6.5",
    "flag-icon-css": "^3.2.1",
    "font-awesome": "^4.7.0",
    "moment": "^2.24.0",
    "mutationobserver-shim": "^0.3.3",
    "ng2-charts": "^2.4.3",
    "ngx-bootstrap": "^7.0.0",
    "ngx-perfect-scrollbar": "^10.1.1",
    "ngx-toastr": "^13.2.1",
    "rxjs": "~6.6.7",
    "simple-line-icons": "^2.5.2",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.13",
    "@angular-eslint/builder": "12.6.1",
    "@angular-eslint/eslint-plugin": "12.6.1",
    "@angular-eslint/eslint-plugin-template": "12.6.1",
    "@angular-eslint/schematics": "12.6.1",
    "@angular-eslint/template-parser": "12.6.1",
    "@angular/cli": "~12.2.13",
    "@angular/compiler-cli": "~12.2.13",
    "@angular/language-service": "~13.3.6",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "angular-cli-ghpages": "1.0.0-rc.2",
    "conventional-changelog-cli": "^2.1.1",
    "eslint": "^7.26.0",
    "husky": "^7.0.0",
    "is-ci": "^3.0.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.7",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "sass-lint": "^1.12.1",
    "ts-node": "~7.0.0",
    "typescript": "4.3.5"
  }
