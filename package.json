{
  "name": "daming-core-ui",
  "version": "1.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production",
    "changelog": "conventiona-l-changelog -p angular -i CHANGELOG.md -s",
    "changelog:all": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "sass": "sass-lint -v -q",
    "push": "ng build --configuration production --baseHref=/daming-core-ui/ && npx angular-cli-ghpages --dir=dist/daming-core-ui",
    "push:ci": "ng deploy --no-silent",
    "postinstall": "ngcc --properties es2015 browser module main --first-only",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "version:patch": "yarn version --patch",
    "version:minor": "yarn version --minor",
    "prepare": "is-ci || husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.3.11",
    "@angular/common": "~13.3.11",
    "@angular/compiler": "~13.3.11",
    "@angular/core": "~13.3.11",
    "@angular/forms": "~13.3.11",
    "@angular/platform-browser": "~13.3.11",
    "@angular/platform-browser-dynamic": "~13.3.11",
    "@angular/router": "~13.3.11",
    "@coreui/coreui": "~2.1.16",
    "@coreui/coreui-plugin-chartjs-custom-tooltips": "^1.3.1",
    "angular-toaster": "^1.0.0",
    "bootstrap": "^4.5.0",
    "chart.js": "^3.8.0",
    "core-js": "^3.6.5",
    "flag-icon-css": "^4.1.7",
    "font-awesome": "^4.7.0",
    "moment": "^2.29.4",
    "mutationobserver-shim": "^0.3.3",
    "ng2-charts": "^3.1.2",
    "ngx-bootstrap": "^7.0.0",
    "ngx-perfect-scrollbar": "^10.1.1",
    "ngx-toastr": "^14.3.0",
    "rxjs": "~6.6.7",
    "simple-line-icons": "^2.5.2",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.8",
    "@angular-eslint/builder": "13.5.0",
    "@angular-eslint/eslint-plugin": "13.5.0",
    "@angular-eslint/eslint-plugin-template": "13.5.0",
    "@angular-eslint/schematics": "14.0.2",
    "@angular-eslint/template-parser": "13.5.0",
    "@angular/cli": "~14.1.1",
    "@angular/compiler-cli": "~13.3.11",
    "@angular/language-service": "~13.3.11",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "angular-cli-ghpages": "1.0.0",
    "conventional-changelog-cli": "^2.1.1",
    "eslint": "^8.2.0",
    "husky": "^7.0.0",
    "is-ci": "^3.0.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.7",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "sass-lint": "^1.12.1",
    "ts-node": "~7.0.0",
    "typescript": "4.6.4"
  }
}
